<div class="form-upload">
    <h1>Urls#new</h1>
    <%= flash[:error] %>
    <%= form_with model: @url do |form| %>
        <div class="form-group">
            <%= form.text_field :name,placeholder:"URL name", :style=>'width:100%'%>
        </div>
        <div class="form-group">
            <%= form.text_field :long_url, placeholder:"URL here!",:style=>'width:100%' %>
        </div>
        <div class="text-center">
            <%= form.submit  class: 'btn btn-success' %>
        </div>
    <% end %>
</div>
